        "SL_path = '/content/String_Lengths: V54 (V)'\n",
        "data = np.genfromtxt(SL_path, delimiter=',')\n",
        "\n",
        "period_guess = data[:,0]\n",
        "string_length = data[:,1]\n",
        "\n",
        "minimum = np.min(string_length)\n",
        "\n",
        "for count, i in enumerate(string_length):\n",
        "  if i == minimum:\n",
        "    period = period_guess[count]\n",
        "\n",
        "fig, ax = plt.subplots(1,1, figsize = [3,3])\n",
        "plt.axvline(period, color= \"red\", linewidth= 0.8, label= \"period: \" + str(period))\n",
