        "# Find Periodogram"
      ],
      "metadata": {
        "id": "wudz85tvhWmj"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "M13 = np.loadtxt(\"/content/V54-(DC) - V.tsv\")"
      ],
      "metadata": {
        "id": "iWDvMmwzhn5D"
      },
      "execution_count": 209,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "MJD = M13[:,0]\n",
        "calibrated_mag = M13[:,1]\n",
        "\n",
        "def find_length(period):\n",
        "  T0 = 0\n",
        "  A = MJD\n",
        "  B = calibrated_mag\n",
        "  phases = foldAt(MJD, period, T0)\n",
        "  length = np.sum(np.hypot(np.diff(np.sort(phases)), np.diff(B[np.argsort(phases)])))\n",
        "  return(length)\n",
        "\n",
        "\n",
        "def string(start, end, step):\n",
        "    data = []\n",
        "    num = np.linspace(start, end,(end-start)\n",
        "                      *int(1/step)+1).tolist()\n",
        "    string = [round(i, 5) for i in num]\n",
        "    for x in string:\n",
        "      A = find_length(x)\n",
        "      if x != 0.0:\n",
        "        data.append([x, A])\n",
        "        print (x, A)\n",
        "        with open(\"String_Lengths: V54 (V)\", 'w', newline='') as file:\n",
        "          csvwriter = csv.writer(file)\n",
        "          csvwriter.writerows(data)\n",
        "\n",
        "print(string(0,1,0.0001))"
      ],
      "metadata": {
        "id": "IfMeauWlifVh"
      },
      "execution_count": null,
      "outputs": []
    },
